     "text": [
      "provide a word to search for vowels:sourabh vivek ligade\n",
      "a was found 2 times\n",
      "e was found 2 times\n",
      "i was found 2 times\n",
      "o was found 1 times\n",
      "u was found 1 times\n"
     ]
    }
   ],
   "source": [
    "vowels=['a','e','i','o','u']\n",
    "word=input('provide a word to search for vowels:')\n",
    "found={}\n",
    "for letter in word:\n",
    "    if letter in vowels:\n",
    "        found.setdefault(letter,0)\n",
    "        found[letter]+=1\n",
    "for k,v in sorted(found.items()):\n",
    "    print(k,'was found',v,'times')"
   ]
